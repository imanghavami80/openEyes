<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="form_header">
        <item name="android:fontFamily">@font/roboto_slab_bold</item>
        <item name="android:textColor">@color/blue_dark</item>
        <item name="android:textSize">@dimen/_20ssp</item>
    </style>

    <style name="link">
        <item name="android:fontFamily">@font/roboto_slab_medium</item>
        <item name="android:textColor">@color/blue_dark</item>
        <item name="android:textSize">@dimen/_12ssp</item>
    </style>

    <style name="normal">
        <item name="android:fontFamily">@font/roboto_slab_regular</item>
        <item name="android:textColor">@color/blue_dark</item>
        <item name="android:textSize">@dimen/_12ssp</item>
    </style>

    <style name="Widget.App.BottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView.Colored">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.BottomNavigationView</item>
    </style>

    <style name="ThemeOverlay.App.BottomNavigationView" parent="">
        <item name="colorPrimary">@color/gray2</item>
        <item name="colorOnPrimary">@color/blue_dark</item>
        <item name="android:fontFamily">@font/roboto_slab_regular</item>
        <item name="android:textColor">@color/blue_dark</item>
    </style>
</resources>