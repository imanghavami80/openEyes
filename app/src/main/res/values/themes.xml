<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.OpenEyes" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your light theme here. -->

        <!-- Status bar color. -->
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">@color/white_light</item>
        <!--To disable dark mode-->
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
        <item name="textAppearanceHeadline6">@style/TextAppearance.Shrine.Headline6</item>
    </style>

    <style name="Theme.OpenEyes" parent="Base.Theme.OpenEyes" />
</resources>